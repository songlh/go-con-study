+			err = t.framer.writeContinuation(flush, s.id, endHeaders, t.hBuf.Next(size))
 		}
 		if err != nil {
 			t.notifyError(err)
@@ -546,14 +550,8 @@ func (t *http2Client) Write(s *Stream, data []byte, opts *Options) error {
