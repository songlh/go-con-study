+package command
+
+import (
+	"fmt"
+	"os"
+	"reflect"
+	"sort"
+	"strings"
+
+	"github.com/bgentry/speakeasy"
+	"github.com/codegangsta/cli"
+	"github.com/coreos/etcd/client"
