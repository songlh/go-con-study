-func argOrStdin(args []string, stdin io.Reader, i int) (string, error) {
-	if i < len(args) {
-		return args[i], nil
-	}
-	bytes, err := ioutil.ReadAll(stdin)
-	if string(bytes) == "" || err != nil {
-		return "", ErrNoAvailSrc
-	}
-	return string(bytes), nil
-}
